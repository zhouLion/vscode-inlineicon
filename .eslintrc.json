{
  "env": {
    "es6": true,
    "node": true,
    "mocha": true
  },
  "extends": [
    "standard"
  ],
  "globals": {
    "NodeJS": "readonly",
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {}
}
